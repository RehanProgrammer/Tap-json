                    "CountyCode" : "$",
                    "StateCode" : "$",
                    "DirectionstoField" : "FIELD_LOC1",
                    "FarmCode" : "FARM_ID",
                    "Lienholders" : "LIENHOLDER",
                    "Contract" : "$",
                    "ImportMatch" : "PROD_ID~~FARM_ID",
