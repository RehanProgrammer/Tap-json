{
    "mapping" : {
        "item":{
            "arrayItems": {
                "objectify": "record",
                "item": {
                    "CustomerRef" : "$2",
                    "last": "$",
                    "first": "$",
                    "FullName": "Name",
                    "TaxNum": "$(no match)",
                    "Address": "ADDRESS1",
                    "Address optional" : "ADDRESS2",
                    "City": "CITY",
                    "State": "STATE",
                    "Zip": "ZIPCODE",
                    "Phone": "PHONE_NUM(NUMBER)",
                    "CustomerCode" : "SDBUYER_ID",
                    "BusinessType" : "$(no match)",
                    "EwrID": "FED_ID",
                    "Email": "EMAIL",
                    "Password": "$(no match)",
                    "Version" : "$(no match)",
		            "ImportMatch" : "$(no match)"
                }
            }
        }
    }
}